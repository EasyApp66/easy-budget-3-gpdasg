
{
  "expo": {
    "name": "Easy Budget 2.0",
    "slug": "Easy Budget 2.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/7a5296a3-50b3-47f8-97c1-290442760300.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.easybudget.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "SKAdNetworkItems": []
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/7a5296a3-50b3-47f8-97c1-290442760300.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.easybudget.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "easybudget",
              "host": "auth-callback"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/images/7a5296a3-50b3-47f8-97c1-290442760300.png",
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      "expo-superwall"
    ],
    "scheme": "easybudget",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "your-project-id"
      },
      "backendUrl": "https://wu7kwypubgksy89mk4h6tatrg56ateqy.app.specular.dev"
    },
    "owner": "anonymous"
  }
}
