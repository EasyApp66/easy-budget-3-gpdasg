{
  "expo": {
    "name": "Easy Budget",
    "slug": "easybudget",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/0c5adf92-17b4-4e1a-96e3-bfbab78549cf.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.easybudget.app",
      "icon": "./assets/images/0c5adf92-17b4-4e1a-96e3-bfbab78549cf.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "SKAdNetworkItems": []
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/0c5adf92-17b4-4e1a-96e3-bfbab78549cf.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.easybudget.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "easybudget",
              "host": "auth-callback"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/images/0c5adf92-17b4-4e1a-96e3-bfbab78549cf.png",
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "easybudget",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "backendUrl": "https://wu7kwypubgksy89mk4h6tatrg56ateqy.app.specular.dev"
    },
    "owner": "anonymous",
    "splash": {
      "image": "./assets/images/0c5adf92-17b4-4e1a-96e3-bfbab78549cf.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    }
  }
}