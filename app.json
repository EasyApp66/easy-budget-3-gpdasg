
{
  "expo": {
    "name": "Easy Budget",
    "slug": "easy-budget",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/0c5adf92-17b4-4e1a-96e3-bfbab78549cf.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/0c5adf92-17b4-4e1a-96e3-bfbab78549cf.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.easybudget.app",
      "buildNumber": "1",
      "icon": "./assets/images/0c5adf92-17b4-4e1a-96e3-bfbab78549cf.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "SKAdNetworkItems": [],
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you.",
        "NSCameraUsageDescription": "Allow Easy Budget to access your camera to take photos.",
        "NSPhotoLibraryUsageDescription": "Allow Easy Budget to access your photo library to select images.",
        "NSPhotoLibraryAddUsageDescription": "Allow Easy Budget to save photos to your photo library."
      },
      "associatedDomains": [
        "applinks:easybudget.app"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/0c5adf92-17b4-4e1a-96e3-bfbab78549cf.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.easybudget.app",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "easybudget",
              "host": "auth-callback"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/images/0c5adf92-17b4-4e1a-96e3-bfbab78549cf.png",
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      "expo-secure-store"
    ],
    "scheme": "easybudget",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "your-eas-project-id"
      }
    },
    "owner": "your-expo-username",
    "description": "Easy Budget 3.0 - Minimalistischer Dark-Mode Budget- und Abo-Tracker mit Premium-Feeling",
    "githubUrl": "https://github.com/yourusername/easy-budget"
  }
}
